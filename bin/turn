#!/usr/bin/env ruby
require_relative '../lib/turn'
board = [" ", "X", " ", "X", " ", "X", " ", "X", " "]
def turn(board)
print "Welcome to Tic Tac Toe!"
puts "Please enter 1-9:"
input = gets.strip

index = input_to_index(input)
move(board,index)
end
puts "Welcome to Tic Tac Toe!"
display_board(board)
